<h1>Song Form</h1>
<%= form_for @song do |f| %>
  <%= label_tag('song[:name]', "Song Name") %>
  <%= f.text_field :name %><br>

  <%= label_tag('song[:artist_id]', "Artist") %>
  <%= f.text_field :artist_id %><br>

  <%= label_tag('song[:genre_id]', "Genre") %>
  <%= f.text_field :genre_id %><br>
 
  <%= f.submit %>
<% end %>